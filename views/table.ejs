<!DOCTYPE html>
<html>
<head>
  <title>JSON Data Table</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }
    
    th, td {
      border: 1px solid black;
      padding: 8px;
      text-align: left;
    }
    
    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Filename</th>
        <th>Link</th>
      </tr>
    </thead>
    <tbody>
      <% jsonData.forEach((item) => { %>
        <tr>
          <td><%= item.title %></td>
          <td><%= item.filename %></td>
          <td><a href="<%= item.link %>"><%= item.link %></a></td>
        </tr>
      <% }); %>
    </tbody>
  </table>
</body>
</html>
